<li class="user">
  <%= image_tag user.avatar_url %>
  <%= link_to user.name, user, class: "name" %>
  <% if current_user.admin? && !current_user?(user) %>
  | <%= link_to "削除", user, data: { "turbo-method": :delete,
                                         turbo_confirm: "削除しますか？" } %>
  <% end %>
</li>